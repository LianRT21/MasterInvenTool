--
-- TOC entry 227 (class 1259 OID 24726)
-- Name: vista_ventas_resumen; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.vista_ventas_resumen AS
 SELECT f.id_factura,
    f.fecha,
    c.nombre AS cliente,
    e.nombre AS empleado,
    f.total
   FROM ((public.facturas f
     JOIN public.clientes c ON ((f.id_cliente = c.id_cliente)))
     JOIN public.empleados e ON ((f.id_empleado = e.id_empleado)))
  ORDER BY f.fecha DESC;


ALTER VIEW public.vista_ventas_resumen OWNER TO postgres;

